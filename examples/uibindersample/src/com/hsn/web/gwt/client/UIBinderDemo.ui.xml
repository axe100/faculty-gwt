<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style src="Uibindersample.css" />
	
	<g:HTMLPanel>
		<h1>Web Application Starter Project</h1>
		
		<table align="center">
			<tr>
				<td colspan="2" style="font-weight:bold;">Please enter your name:</td>
			</tr>
			<tr>
				<td id="nameFieldContainer"><g:TextBox ui:field="nameField" focus="true">GWT User</g:TextBox></td>
				<td id="sendButtonContainer"><g:Button ui:field="sendButton" addStyleNames="{style.sendButton}">Send</g:Button></td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder>